{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "root": "client",
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "nodeModulesPath": "client/node_modules"
    },
    "preview": {
      "distribution": "internal",
      "nodeModulesPath": "client/node_modules"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "nodeModulesPath": "client/node_modules"
    }
  },
  "submit": {
    "production": {}
  }
}
