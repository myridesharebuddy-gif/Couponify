{
  "name": "couponify-client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "set:dev-ip": "node ../scripts/set-dev-ip.mjs",
    "dev": "EXPO_PACKAGER_PORT=19001 npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "build": "npx expo build:web",
    "fix:expo": "npx expo doctor && npx expo install --fix && npx expo start -c",
    "clean:hard": "rm -rf node_modules .expo ios android yarn.lock package-lock.json && npm cache clean --force",
    "rebuild:expo": "npm run clean:hard && npm install && npx expo install --fix",
    "eas:ios": "npx eas build --platform ios --profile production",
    "start": "expo start",
    "prebuild": "expo prebuild"
  },
  "dependencies": {
    "@expo-google-fonts/cormorant-garamond": "^0.3.0",
    "@expo-google-fonts/dancing-script": "^0.4.2",
    "@expo-google-fonts/great-vibes": "^0.2.0",
    "@expo-google-fonts/playfair-display": "^0.3.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.12",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^4.36.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "0.72.0",
    "@types/react-native-vector-icons": "^6.4.13",
    "dotenv": "^16.4.5",
    "expo": "~49.0.11",
    "expo-barcode-scanner": "~12.5.0",
    "expo-camera": "~13.4.4",
    "expo-clipboard": "~4.3.1",
    "expo-constants": "~14.4.2",
    "expo-font": "~11.4.0",
    "expo-modules-core": "~1.5.12",
    "expo-notifications": "~0.20.1",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.4",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-get-random-values": "~1.9.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-vector-icons": "^9.2.0",
    "uuid": "^10.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@expo/cli": "^54.0.21",
    "typescript": "^5.5.8"
  },
  "main": "expo/entry"
}
